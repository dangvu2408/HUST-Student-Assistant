<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ctt_hust_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ctt_hust_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme1"
        tools:targetApi="31">
        <activity
            android:name=".AppCompat.Class_Info"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".AppCompat.Hust_Map"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".AppCompat.Splash"
            android:exported="false"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AppCompat.MainActivity2"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".AppCompat.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".LoginPropeties.LoginActivity"
            android:exported="true"
            android:screenOrientation="portrait"/>

    </application>

</manifest>